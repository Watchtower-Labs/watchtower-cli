{
	"name": "@watchtower/cli",
	"version": "0.1.0",
	"description": "Terminal UI for viewing ADK agent traces",
	"license": "MIT",
	"author": "Watchtower Contributors",
	"repository": {
		"type": "git",
		"url": "https://github.com/Watchtower-Labs/watchtower-cli.git",
		"directory": "packages/cli"
	},
	"homepage": "https://github.com/Watchtower-Labs/watchtower-cli#readme",
	"bugs": {
		"url": "https://github.com/Watchtower-Labs/watchtower-cli/issues"
	},
	"keywords": [
		"cli",
		"terminal",
		"adk",
		"agent",
		"observability",
		"tracing",
		"google-adk",
		"llm",
		"ai-agents",
		"ink",
		"react"
	],
	"bin": {
		"watchtower": "dist/index.js"
	},
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "tsc --project tsconfig.test.json && ava dist/*.test.js",
		"test:watch": "ava --watch dist/*.test.js",
		"lint": "prettier --check . && xo",
		"lint:fix": "prettier --write . && xo --fix",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"files": [
		"dist",
		"README.md"
	],
	"dependencies": {
		"chalk": "^5.3.0",
		"ink": "^4.4.1",
		"react": "^18.2.0",
		"uuid": "^9.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^20.0.0",
		"@types/react": "^18.2.0",
		"@types/uuid": "^9.0.0",
		"@types/yargs": "^17.0.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.0",
		"xo": "^0.53.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"ava": {
		"files": [
			"dist/*.test.js"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
